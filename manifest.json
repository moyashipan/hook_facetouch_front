{
	"manifest_version": 2,

	"name": "HookFaceTouch",
	"description": "FaceTouchからのHTTPリクエストをフックしてIRCに通知を行います",
	"version": "0.1",

	"browser_action": {
	},
	"background": {
		"scripts": ["lib/jquery.min.js", "background.js"]
	},
	"permissions": [
		"webRequest",
		"https://www.facetouch.jp/*"
	]
}

